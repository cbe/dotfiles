// See these two files for default bindings
// https://github.com/codebling/vs-code-default-keybindings/blob/master/linux.keybindings.json
// https://github.com/codebling/vs-code-default-keybindings/blob/master/macos.keybindings.json
[
  // Panes
  { "key": "shift+cmd+e", "command": "-workbench.view.explorer" },
  { "key": "ctrl+alt+shift+e", "command": "workbench.view.explorer", "when": "viewContainer.workbench.view.explorer.enabled" },
  { "key": "ctrl+shift+e", "command": "workbench.files.action.focusOpenEditorsView", "when": "workbench.explorer.openEditorsView.active" },
  { "key": "ctrl+shift+e", "command": "workbench.action.focusPreviousGroup", "when": "focusedView == 'workbench.explorer.openEditorsView'" },
  { "key": "shift+cmd+x", "command": "-workbench.view.extensions" },
  { "key": "ctrl+shift+x", "command": "workbench.view.extensions", "when": "viewContainer.workbench.view.extensions.enabled" },
  { "key": "shift+cmd+f", "command": "-workbench.view.search" },
  { "key": "ctrl+shift+f", "command": "workbench.view.search", "when": "workbench.view.search.active && neverMatch =~ /doesNotMatch/" },
  { "key": "ctrl+shift+r", "command": "workbench.view.extension.references-view" },
  { "key": "cmd+j", "command": "-workbench.action.togglePanel" },
  { "key": "alt+p", "command": "workbench.action.togglePanel" },
  { "key": "alt+shift+p", "command": "workbench.action.toggleMaximizedPanel" },

  // Workspace
  { "key": "ctrl+w", "command": "-workbench.action.switchWindow" },
  { "key": "ctrl+w", "command": "workbench.action.closeActiveEditor" },
  { "key": "shift+cmd+t", "command": "-workbench.action.reopenClosedEditor" },
  { "key": "ctrl+shift+w", "command": "workbench.action.reopenClosedEditor" },
  { "key": "shift+cmd+f", "command": "-workbench.action.findInFiles" },
  { "key": "ctrl+shift+f", "command": "workbench.action.findInFiles" },
  { "key": "cmd+f", "command": "-actions.find" },
  { "key": "ctrl+f", "command": "actions.find", "when": "editorFocus || editorIsOpen" },
  { "key": "ctrl+f", "command": "workbench.action.focusPreviousGroup", "when": "findInputFocussed" },
  { "key": "cmd+p", "command": "-workbench.action.quickOpen" },
  { "key": "ctrl+p", "command": "workbench.action.quickOpen" },
  { "key": "shift+cmd+p", "command": "-workbench.action.showCommands" },
  { "key": "ctrl+shift+p", "command": "workbench.action.showCommands" },
  { "key": "ctrl+=", "command": "-workbench.action.zoomIn" },
  { "key": "ctrl+shift+=", "command": "-workbench.action.zoomIn" },
  { "key": "ctrl+numpad_add", "command": "-workbench.action.zoomIn" },
  { "key": "cmd+=", "command": "-workbench.action.zoomIn" },
  { "key": "ctrl+-", "command": "-workbench.action.zoomOut" },
  { "key": "ctrl+numpad_subtract", "command": "-workbench.action.zoomOut" },
  { "key": "cmd+-", "command": "-workbench.action.zoomOut" },
  { "key": "ctrl+q", "command": "workbench.action.navigateBack" },
  { "key": "ctrl+'", "command": "workbench.action.navigateForward" },
  { "key": "ctrl+tab", "command": "-workbench.action.quickOpenPreviousRecentlyUsedEditorInGroup" },
  { "key": "ctrl+tab", "command": "workbench.action.nextEditor" },
  { "key": "ctrl+tab", "command": "workbench.action.terminal.focusNext", "when": "terminalFocus && terminalProcessSupported && !terminalEditorFocus" },
  { "key": "ctrl+shift+tab", "command": "-workbench.action.quickOpenLeastRecentlyUsedEditorInGroup" },
  { "key": "ctrl+shift+tab", "command": "workbench.action.previousEditor" },
  { "key": "ctrl+shift+tab", "command": "workbench.action.terminal.focusPrevious", "when": "terminalFocus && terminalProcessSupported && !terminalEditorFocus" },
  { "key": "ctrl+\\", "command": "-workbench.action.splitEditor" },
  { "key": "cmd+\\", "command": "-workbench.action.splitEditor" },
  { "key": "ctrl+\\", "command": "workbench.action.splitEditor", "when": "!multipleEditorGroups" },
  { "key": "ctrl+\\", "command": "workbench.action.joinAllGroups", "when": "multipleEditorGroups" },
  { "key": "ctrl+pageup", "command": "-workbench.action.previousEditor" },
  { "key": "ctrl+pageup", "command": "workbench.action.focusLeftGroup" },
  { "key": "ctrl+pageup", "command": "workbench.action.focusActiveEditorGroup", "when": "terminalFocus" },
  { "key": "ctrl+pagedown", "command": "-workbench.action.nextEditor" },
  { "key": "ctrl+pagedown", "command": "workbench.action.focusRightGroup" },
  { "key": "ctrl+pagedown", "command": "workbench.action.focusPanel", "when": "activeEditorGroupLast" },
  { "key": "ctrl+shift+t", "command": "workbench.action.terminal.new", "when": "!isMac && terminalFocus" },
  { "key": "ctrl+shift+w", "command": "workbench.action.terminal.kill", "when": "!isMac && terminalFocus" },
  { "key": "ctrl+cmd+left", "command": "-workbench.action.terminal.resizePaneLeft" },
  { "key": "ctrl+cmd+right", "command": "-workbench.action.terminal.resizePaneRight" },
  { "key": "ctrl+shift+left", "command": "workbench.action.terminal.resizePaneLeft", "when": "terminalFocus && terminalHasBeenCreated || terminalFocus && terminalProcessSupported" },
  { "key": "ctrl+shift+right", "command": "workbench.action.terminal.resizePaneRight", "when": "terminalFocus && terminalHasBeenCreated || terminalFocus && terminalProcessSupported" },
  { "command": "-workbench.action.focusSideBar" },
  { "command": "-workbench.action.focusFirstEditorGroup" },
  { "command": "-workbench.action.focusSecondEditorGroup" },
  { "command": "-workbench.action.focusThirdEditorGroup" },
  { "command": "-workbench.action.focusFourthEditorGroup" },
  { "command": "-workbench.action.focusFifthEditorGroup" },
  { "command": "-workbench.action.focusSixthEditorGroup" },
  { "command": "-workbench.action.focusSeventhEditorGroup" },
  { "command": "-workbench.action.focusEighthEditorGroup" },
  { "command": "-workbench.action.lastEditorInGroup" },

  // Open editors
  { "key": "ctrl+shift+pageup", "command": "-workbench.action.moveEditorLeftInGroup" },
  { "key": "ctrl+shift+pagedown", "command": "-workbench.action.moveEditorRightInGroup" },
  { "key": "alt+up", "command": "workbench.action.moveEditorLeftInGroup", "when": "focusedView == 'workbench.explorer.openEditorsView'" },
  { "key": "alt+down", "command": "workbench.action.moveEditorRightInGroup", "when": "focusedView == 'workbench.explorer.openEditorsView'" },

  // File
  { "key": "shift+cmd+;", "command": "-breadcrumbs.focus" },
  { "key": "ctrl+shift+.", "command": "-breadcrumbs.focusAndSelect" },
  { "key": "ctrl+shift+o", "command": "breadcrumbs.focus", "when": "breadcrumbsPossible && breadcrumbsVisible" },
  { "key": "cmd+z", "command": "-undo" },
  { "key": "ctrl+z", "command": "undo" },
  { "key": "ctrl+y", "command": "-redo" },
  { "key": "shift+cmd+z", "command": "-redo" },
  { "key": "ctrl+shift+z", "command": "redo" },
  { "key": "cmd+s", "command": "-workbench.action.files.save" },
  { "key": "ctrl+s", "command": "workbench.action.files.save" },

  // Clipboard
  { "key": "cmd+c", "command": "-editor.action.clipboardCopyAction" },
  { "key": "ctrl+c", "command": "editor.action.clipboardCopyAction" },
  { "key": "cmd+x", "command": "-editor.action.clipboardCutAction" },
  { "key": "ctrl+x", "command": "editor.action.clipboardCutAction" },
  { "key": "cmd+v", "command": "-editor.action.clipboardPasteAction" },
  { "key": "ctrl+v", "command": "editor.action.clipboardPasteAction" },

  // Scrolling
  { "key": "alt+pageup", "command": "scrollPageUp", "when": "textInputFocus" },
  { "key": "alt+pagedown", "command": "scrollPageDown", "when": "textInputFocus" },

  // Line operations
  { "key": "shift+cmd+enter", "command": "-editor.action.insertLineBefore" },
  { "key": "ctrl+shift+enter", "command": "editor.action.insertLineBefore","when": "editorTextFocus && !editorReadonly" },
  { "key": "cmd+enter", "command": "-editor.action.insertLineAfter" },
  { "key": "ctrl+enter", "command": "editor.action.insertLineAfter", "when": "editorTextFocus && !editorReadonly" },
  { "key": "shift+cmd+k", "command": "-editor.action.deleteLines" },
  { "key": "ctrl+shift+k", "command": "editor.action.deleteLines", "when": "textInputFocus && !editorReadonly" },
  { "key": "ctrl+j", "command": "editor.action.joinLines", "when": "editorTextFocus && !editorReadonly" },
  { "key": "ctrl+shift+d", "command": "-workbench.view.debug", "when": "viewContainer.workbench.view.debug.enabled" },
  { "key": "ctrl+shift+d", "command": "editor.action.duplicateSelection" },
  { "key": "cmd+shift+d", "command": "-workbench.view.debug", "when": "viewContainer.workbench.view.debug.enabled" },
  { "key": "ctrl+up", "command": "cursorMove", "when": "editorTextFocus", "args": { "to": "prevBlankLine", "by": "wrappedLine" } },
  { "key": "ctrl+shift+up", "command": "cursorMove", "when": "editorTextFocus", "args": { "to": "prevBlankLine", "by": "wrappedLine", "select": true } },
  { "key": "ctrl+down", "command": "cursorMove", "when": "editorTextFocus", "args": { "to": "nextBlankLine", "by": "wrappedLine" } },
  { "key": "ctrl+shift+down", "command": "cursorMove", "when": "editorTextFocus", "args": { "to": "nextBlankLine", "by": "wrappedLine", "select": true } },
  { "key": "shift+cmd+\\", "command": "-editor.action.jumpToBracket" },
  { "key": "ctrl+shift+b", "command": "editor.action.jumpToBracket", "when": "editorTextFocus" },
  { "key": "alt+cmd+backspace", "command": "-editor.action.removeBrackets" },
  { "key": "ctrl+alt+backspace", "command": "-editor.action.removeBrackets" },
  { "key": "ctrl+alt+shift+b", "command": "editor.action.removeBrackets", "when": "editorTextFocus" },

  // Word operations
  { "key": "ctrl+k ctrl+u", "command": "editor.action.transformToUppercase" },
  { "key": "ctrl+k ctrl+l", "command": "editor.action.transformToLowercase" },

  // Selection
  { "key": "ctrl+d", "command": "-deleteRight" },
  { "key": "ctrl+d", "command": "editor.action.addSelectionToNextFindMatch", "when": "editorFocus" },
  { "key": "shift+cmd+l", "command": "-editor.action.selectHighlights" },
  { "key": "ctrl+shift+l", "command": "editor.action.selectHighlights", "when": "editorFocus" },
  { "key": "cmd+/", "command": "-editor.action.commentLine" },
  { "key": "ctrl+/", "command": "editor.action.commentLine", "when": "editorTextFocus && !editorReadonly" },
  { "key": "shift+alt+up", "command": "editor.action.insertCursorAbove", "when": "editorTextFocus" },
  { "key": "shift+alt+down", "command": "editor.action.insertCursorBelow", "when": "editorTextFocus" },
  { "key": "alt+cmd+up", "command": "-editor.action.insertCursorAbove" },
  { "key": "alt+cmd+down", "command": "-editor.action.insertCursorBelow" },
  { "key": "ctrl+alt+shift+left", "command": "cursorWordPartLeftSelect", "when": "textInputFocus" },
  { "key": "ctrl+alt+shift+right", "command": "cursorWordPartRightSelect", "when": "textInputFocus" },
  { "key": "shift+alt+left", "command": "-editor.action.smartSelect.shrink", "when": "editorTextFocus && (isLinux || isWindows)" },
  { "key": "ctrl+shift+left", "command": "-editor.action.smartSelect.shrink" },
  { "key": "ctrl+alt+shift+up", "command": "editor.action.smartSelect.shrink", "when": "editorTextFocus" },
  { "key": "shift+alt+right", "command": "-editor.action.smartSelect.expand", "when": "editorTextFocus && (isLinux || isWindows)" },
  { "key": "ctrl+shift+right", "command": "-editor.action.smartSelect.expand" },
  { "key": "ctrl+alt+shift+down", "command": "editor.action.smartSelect.expand", "when": "editorTextFocus" },

  // Suggestions
  { "key": "cmd+i", "command": "-editor.action.triggerSuggest" },
  { "key": "ctrl+shift+space", "command": "editor.action.triggerParameterHints", "when": "editorHasSignatureHelpProvider && editorTextFocus" },

  // Symbols
  { "key": "ctrl+shift+g", "command": "editor.action.revealDefinition", "when": "editorHasDefinitionProvider && editorTextFocus" },
  { "key": "shift+f7", "command": "-editor.action.wordHighlight.prev" },
  { "key": "alt+left", "command": "editor.action.wordHighlight.prev", "when": "editorTextFocus && hasWordHighlights" },
  { "key": "f7", "command": "-editor.action.wordHighlight.next" },
  { "key": "alt+right", "command": "editor.action.wordHighlight.next", "when": "editorTextFocus && hasWordHighlights" },
  { "key": "shift+alt+f12", "command": "-references-view.findReferences" },
  { "key": "ctrl+alt+shift+g", "command": "references-view.findReferences", "when": "editorHasReferenceProvider" },
  { "key": "cmd+t", "command": "-workbench.action.showAllSymbols" },
  { "key": "ctrl+shift+t", "command": "workbench.action.showAllSymbols" },
  { "key": "shift+cmd+o", "command": "-workbench.action.gotoSymbol" },
  { "key": "ctrl+t", "command": "workbench.action.gotoSymbol", "when": "!accessibilityHelpIsShown && !accessibleViewIsShown" },

  // Markers
  { "key": "shift+alt+f5", "command": "-workbench.action.editor.previousChange" },
  { "key": "shift+alt+left", "command": "workbench.action.editor.previousChange", "when": "editorTextFocus && !textCompareEditorActive && quickDiffDecorationCount != '0'" },
  { "key": "alt+f5", "command": "-workbench.action.editor.nextChange" },
  { "key": "ctrl+m", "command": "-editor.action.toggleTabFocusMode" },
  { "key": "ctrl+shift+m", "command": "-editor.action.toggleTabFocusMode" },
  { "key": "shift+alt+right", "command": "workbench.action.editor.nextChange", "when": "editorTextFocus && !textCompareEditorActive && quickDiffDecorationCount != '0'" },
  { "key": "shift+f3", "command": "-editor.action.previousMatchFindAction" },
  { "key": "shift+alt+a", "command": "editor.action.previousMatchFindAction", "when": "editorFocus" },
  { "key": "f3", "command": "-editor.action.nextMatchFindAction" },
  { "key": "shift+alt+i", "command": "editor.action.nextMatchFindAction", "when": "editorFocus" },
  { "key": "f8", "command": "-editor.action.marker.nextInFiles" },
  { "key": "ctrl+y", "command": "editor.action.marker.nextInFiles", "when": "editorFocus" },
  { "key": "shift+f8", "command": "-editor.action.marker.prevInFiles" },
  { "key": "ctrl+shift+y", "command": "editor.action.marker.prevInFiles", "when": "editorFocus" },

  // macOS
  { "key": "cmd+n", "command": "workbench.action.files.newUntitledFile" },
  { "key": "ctrl+n", "command": "workbench.action.files.newUntitledFile", "when": "isMac" },
  { "key": "shift+cmd+n", "command": "workbench.action.newWindow" },
  { "key": "ctrl+shift+n", "command": "workbench.action.newWindow", "when": "isMac" },
  { "key": "alt+left", "command": "-cursorWordLeft" },
  { "key": "ctrl+left", "command": "cursorWordLeft", "when": "textInputFocus && isMac" },
  { "key": "alt+right", "command": "-cursorWordEndRight" },
  { "key": "ctrl+right", "command": "cursorWordEndRight", "when": "textInputFocus && isMac" },
  { "key": "shift+alt+left", "command": "-cursorWordLeftSelect" },
  { "key": "ctrl+shift+left", "command": "cursorWordLeftSelect", "when": "textInputFocus" },
  { "key": "shift+alt+right", "command": "-cursorWordEndRightSelect" },
  { "key": "ctrl+shift+right", "command": "cursorWordEndRightSelect", "when": "textInputFocus" },
  { "key": "alt+backspace", "command": "-deleteWordLeft" },
  { "key": "ctrl+backspace", "command": "deleteWordLeft", "when": "textInputFocus && !editorReadonly && isMac" },
  { "key": "cmd+i", "command": "-inlineChat.start", "when": "editorFocus && inlineChatHasEditsAgent && inlineChatPossible && !editorReadonly && !editorSimpleInput || editorFocus && inlineChatHasProvider && inlineChatPossible && !editorReadonly && !editorSimpleInput" },
  { "key": "cmd+i", "command": "-inlineChat.startWithCurrentLine", "when": "inlineChatHasProvider && inlineChatShowingHint && !editorReadonly && !inlineChatVisible" },
  { "key": "cmd+i", "command": "-workbench.action.terminal.chat.start", "when": "chatIsEnabled && terminalChatAgentRegistered && terminalFocusInAny && terminalHasBeenCreated || chatIsEnabled && terminalChatAgentRegistered && terminalFocusInAny && terminalProcessSupported" },
  { "key": "cmd+i", "command": "-inlineChat2.reveal", "when": "inlineChatHasEditsAgent && !chatEdits.isGlobalEditingSession && chatEdits.requestCount >= 1" }
]
